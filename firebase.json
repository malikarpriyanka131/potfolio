{
  "hosting": {
  "public": "dist/myPortfolio/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.pdf",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/pdf"
          },
          {
            "key": "Content-Disposition",
            "value": "attachment"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "predeploy": ["npm --prefix . run build"]
  },
  "emulators": {
    "dataconnect": {
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": "dataconnect"
  }
}
